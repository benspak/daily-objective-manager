{
  "manifest_version": 3,
  "name": "Daily Objectives Manager",
  "version": "1.0",
  "description": "Log your top 5 daily objectives and view past objectives.",
  "permissions": ["storage", "alarms"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "history.html"
  }
}
