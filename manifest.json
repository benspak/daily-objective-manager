{
  "manifest_version": 3,
  "name": "Daily Objectives Tracker",
  "version": "1.1",
  "description": "Track your top 5 daily objectives and view past objectives.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["history.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
